{
  "dependencies": {
    "@rdfjs/types": "^1.1.0",
    "@types/n3": "^1.16.4",
    "@types/unbzip2-stream": "^1.4.3",
    "base-x": "^3.0.9",
    "envalid": "^8.0.0",
    "mime": "^4.0.4",
