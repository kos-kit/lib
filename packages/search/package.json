{
  "dependencies": {
    "@kos-kit/mem-models": "2.0.29",
    "@kos-kit/models": "2.0.29",
    "@kos-kit/rdf-resource": "2.0.29",
    "@rdfjs/types": "^1.1.0",
    "@types/lunr": "^2.3.7",
    "axios": "^1.7.2",
    "lunr": "^2.3.9"
  },
  "engines": {
    "node": ">=20"
  },
  "main": "index.js",
  "files": [
    "*.d.ts",
    "*.js"
  ],
  "license": "Apache-2.0",
  "name": "@kos-kit/search",
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf *.d.ts* *.js *.js.map __tests__/*.d.ts* __tests__/*.js __tests__/*.js.map",
    "format": "biome check --write --unsafe .",
    "lint": "tsc -b --clean && biome lint",
    "rebuild": "run-s clean build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "unlink": "npm unlink -g @kos-kit/search",
    "watch": "tsc -w --preserveWatchOutput"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kos-kit/lib"
  },
  "type": "module",
  "types": "index.d.ts",
  "version": "2.0.29"
}
